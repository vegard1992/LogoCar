	signal ram: ram_type := (2 => x"D0", 3 => x"32", 4 => x"AA", 5 => x"5A", 6 => x"D0", 7 => x"32", 8 => x"AA", 9 => x"5A", 10 => x"D0", 11 => x"32", 12 => x"AA", 13 => x"5A", 14 => x"D0", 15 => x"32", 16 => x"55", 17 => x"5A", 18 => x"D0", 19 => x"32", 20 => x"55", 21 => x"5A", 22 => x"D0", 23 => x"32", 24 => x"55", 25 => x"5A", 26 => x"D0", 27 => x"32", others => x"FF");